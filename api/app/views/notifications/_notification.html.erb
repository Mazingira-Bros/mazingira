<div id="<%= dom_id notification %>">
  <p>
    <strong>Title:</strong>
    <%= notification.title %>
  </p>

  <p>
    <strong>Message:</strong>
    <%= notification.message %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= notification.date %>
  </p>

  <p>
    <strong>Sender:</strong>
    <%= notification.sender %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= notification.image %>
  </p>

</div>
